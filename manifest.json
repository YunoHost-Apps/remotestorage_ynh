{
    "name": "remoteStorage",
    "id": "remotestorage",
    "packaging_format": 1,
    "description": {
        "en": "This is a remoteStorage server implementation written in PHP.",
        "fr": "This is a remoteStorage server implementation written in PHP."
    },
    "version": "1.0.5~ynh1",
    "url": "https://github.com/fkooman/php-remote-storage",
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Anmol Sharma",
        "email": "anmol@datamol.org"
    },
    "requirements": {
        "yunohost": ">> 4.2.4"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            },
            {
                "name": "password",
                "type": "password",
                "example": "mystrongpassword"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
